						"     input(",
						"          emp_key as integer,",
						"          emp_id as integer,",
						"          fname as string,",
						"          lname as string,",
						"          DOB as date,",
						"          mobile as string,",
						"          gender as string,",
						"          address as string,",
						"          city as string,",
