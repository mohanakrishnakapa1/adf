										"value": "@{activity('Lkp_present_watermark').output.firstRow.newval}",
										"type": "Expression"
									},
									"type": "DateTime"
								},
								"vtblname": {
									"value": {
										"value": "@pipeline().parameters.TabName",
										"type": "Expression"
									},
									"type": "String"
								}
							}
						},
						"linkedServiceName": {
							"referenceName": "AzureSqlDatabase1",
							"type": "LinkedServiceReference"
						}
					}
				],
