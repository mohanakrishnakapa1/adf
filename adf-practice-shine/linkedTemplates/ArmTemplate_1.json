							"source": {
								"type": "AzureSqlSource",
								"sqlReaderQuery": {
									"value": "select * from @{pipeline().parameters.TabName} where\nlastmodifytime >'@{activity('Lkp_Old_waterMarkDate').output.firstRow.oldval}'\nand \nlastmodifytime <='@{activity('Lkp_present_watermark').output.firstRow.newval}'",
